{
    "name": "Real Python Content Loader",
    "description": "Allow Unregistered User to View the Tutorial Page",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "/images/py_icon_transp.png",
            "48": "/images/py_icon_transp2.png",
            "32": "/images/py_icon_transp3.png",
            "16": "/images/py_icon_transp4.png"
        }
    },
    "icons": {
        "128": "/images/py_icon_transp.png",
        "48": "/images/py_icon_transp2.png",
        "32": "/images/py_icon_transp3.png",
        "16": "/images/py_icon_transp4.png"
    }
}